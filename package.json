{
  "name": "xlsx-json-converter-fullstack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www",
    "server": "nodemon ./server/bin/www",
    "client": "npm run start --prefix xlsx-json-converter",
    "client:install": "npm install --prefix xlsx-json-converter",
    "client:build": "npm run build --prefix xlsx-json-converter",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nconf": "^0.11.2",
    "pug": "2.0.0-beta11",
    "winston": "^3.3.3",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "eslint": "^7.24.0",
    "eslint-config-google": "^0.14.0"
  }
}
